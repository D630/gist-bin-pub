<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">
   <keyboard>
    <keybind key="W-F1">
      <action name="Execute">
        <execute>xtermj1.sh</execute>
      </action>
    </keybind>
  </keyboard>
  <applications>
    <application name="xtermj1">
       <decor>no</decor>
       <position force="yes">
         <x>0</x>
         <y>0</y>
       </position>
       <size>
        <width>100%</width>
        <height>75%</height>
       </size>
       <desktop>all</desktop>
       <focus>yes</focus>
       <iconic>no</iconic>
       <layer>above</layer>
       <shade>no</shade>
       <skip_pager>yes</skip_pager>
       <skip_taskbar>yes</skip_taskbar>
       <maximized>false</maximized>
       <fullscreen>no</fullscreen>
    </application>
  </applications>
</openbox_config>
